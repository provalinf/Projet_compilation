/* Combinaison de nombres à 3 chiffres */
programme unPeuDeCombi debut
    entier a;
    entier b;
    entier c;
    entier nb;

    a = 0;
    nb = 0;
    tantque a != 8 repeter
        b = a + 1;
        tantque b != 9 repeter
            c = b + 1;
            tantque c != 10 repeter
                // ecrire la combinaison sous la forme (abc)
                ecrire "(";
                ecrire a;
                ecrire b;
                ecrire c;
                ecrire ")";
                ecrire " ";
                c = c + 1;
                nb = nb + 1;
             fintantque
             b = b + 1;
         fintantque
         a = a + 1;
    fintantque
    ecrire "\nJ'ai trouvé ";
    ecrire nb; // 120 normalement
    ecrire " combinaisons.";
fin
